@inproceedings{zhang_odvba-c:_2013,
	title = {{ODVBA-C:} Optimally-Discriminative Voxel-Based Analysis of Continuous Variables},
	shorttitle = {{ODVBA-C}},
	doi = {10.1109/PRNI.2013.49},
	abstract = {In this paper, we propose a new method that utilizes a novel spatially adaptive scheme for detection of multivariate neuroimaging patterns relating to a continuous subject-level variable, aiming to effectively determine the optimal spatially adaptive filtering of neuroimaging data from the perspective of finding relationships between imaging and continues (e.g. clinical and cognitive) variables. Analyses employ local pattern analysis using regularized least square regression with nonnegativity constraints within a spatial neighborhood around each voxel. Within each neighborhood, we determine the optimal regression coefficients that relate local patterns to the continuous variable of interest. As each voxel belongs to multiple overlapping neighborhoods, the statistic for a given voxel is determined by combining weights from all neighborhoods to which the voxel participates. Finally, non-parametric permutation testing is used to obtain a voxelwise significance map. Using both simulated and real {fMRI} data, we demonstrate the effectiveness of the proposed method.},
	booktitle = {2013 International Workshop on Pattern Recognition in Neuroimaging ({PRNI)}},
	author = {Zhang, Tianhao and Satterthwaite, {T.D.} and Davatzikos, C.},
	year = {2013},
	keywords = {adaptive filters, biomedical {MRI}, Brain modeling, continuous subject-level variable, filtering theory, {fMRI}, {fMRI} data, Imaging, Kernel, local pattern analysis, medical image processing, multiple overlapping neighborhoods, multivariate neuroimaging pattern detection, Neuroimaging, neuroimaging data, neurophysiology, Nonnegativity, nonnegativity constraints, nonparametric permutation testing, {ODVBA}, {ODVBA-C}, optimally-discriminative voxel-based analysis of continuous variables, optimal regression coefficients, regression, regression analysis, regularized least square regression, Smoothing methods, spatial adaptive filtering, spatial adaptive scheme, spatial neighborhood, Standards, statistical testing, vectors, voxelwise significance map},
	pages = {161--164},
	file = {IEEE Xplore Abstract Record:/home/ben/.zotero/zotero/yu6itdki.default/zotero/storage/VTVXWBJD/Zhang et al. - 2013 - ODVBA-C Optimally-Discriminative Voxel-Based Anal.html:text/html;IEEE Xplore Full Text PDF:/home/ben/.zotero/zotero/yu6itdki.default/zotero/storage/7VGBJ6MD/Zhang et al. - 2013 - ODVBA-C Optimally-Discriminative Voxel-Based Anal.pdf:application/pdf}
