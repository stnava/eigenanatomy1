	author = {Coenen, Volker Arnd and Krings, Timo and Axer, Hubertus and Weidemann, Jürgen and Kränzlein, Heidi and Hans, Franz-Josef and Thron, Armin and Gilsbach, Joachim Michael and Rohde, Veit},
	month = nov,
	year = {2003},
	note = {{PMID:} 14572954},
	keywords = {Adult, Aged, Aged, 80 and over, Anisotropy, Brain Neoplasms, Diffusion Magnetic Resonance Imaging, Electric Stimulation, Electrophysiology, Female, Humans, Intraoperative Period, Male, Middle Aged, Motor Cortex, Predictive Value of Tests, Prospective Studies, Pyramidal Tracts},
	pages = {381--390; discussion 390}
},

@article{liu_maternal_1997,
