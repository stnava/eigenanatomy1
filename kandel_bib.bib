	note = {{PMID:} 15220777},
	keywords = {Adhesiveness, Algorithms, Animals, Embolism, Air, Endothelium, Vascular, Fluorocarbons, Male, Mesenteric Arteries, Poloxamer, Rats, Surface-Active Agents},
