	author = {Zhan, Y. and Shen, D.},
	year = {2006},
	keywords = {Deformable segmentation, Gabor filter bank, Gabor-Support Vector Machines, Kernel support vector machine, Prostate segmentation, Tissue differentiation, Transrectal ultrasound image},
	pages = {256--272}
