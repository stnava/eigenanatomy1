	journal = {Alzheimer's \& Dementia},
	author = {Schuff, Norbert and Matsumoto, Shinji and Kmiecik, Joseph and Studholme, Colin and Du, Antao and Ezekiel, Frank and Miller, Bruce L. and Kramer, Joel H. and Jagust, William J. and Chui, Helen C. and Weiner, Michael W.},
