@article{shaffer_predicting_2013,
	title = {Predicting Cognitive Decline in Subjects at Risk for Alzheimer Disease by Using Combined Cerebrospinal Fluid, {MR} Imaging, and {PET} Biomarkers},
	volume = {266},
	issn = {0033-8419, 1527-1315},
	url = {http://radiology.rsna.org/content/266/2/583},
	doi = {10.1148/radiol.12120010},
	abstract = {Purpose: To assess the extent to which multiple Alzheimer disease ({AD)} biomarkers improve the ability to predict future decline in subjects with mild cognitive impairment ({MCI)} compared with predictions based on clinical parameters alone.
Materials and Methods: All protocols were approved by the institutional review board at each site, and written informed consent was obtained from all subjects. The study was {HIPAA} compliant. Alzheimer’s Disease Neuroimaging Initiative ({ADNI)} baseline magnetic resonance ({MR)} imaging and fluorine 18 fluorodeoxyglucose ({FDG)} positron emission tomography ({PET)} studies for 97 subjects with {MCI} were used. {MR} imaging–derived gray matter probability maps and {FDG} {PET} images were analyzed by using independent component analysis, an unbiased data-driven method to extract independent sources of information from whole-brain data. The loading parameters for all {MR} imaging and {FDG} components, along with cerebrospinal fluid ({CSF)} proteins, were entered into logistic regression models (dependent variable: conversion to {AD} within 4 years). Eight models were considered, including all combinations of {MR} imaging, {PET}, and {CSF} markers with the covariates (age, education, apolipoprotein E genotype, Alzheimer’s Disease Assessment Scale-Cognitive subscale score).
Results: Combining {MR} imaging, {FDG} {PET}, and {CSF} data with routine clinical tests significantly increased the accuracy of predicting conversion to {AD} compared with clinical testing alone. The misclassification rate decreased from 41.3\% to 28.4\% (P {\textless} .00001). {FDG} {PET} contributed more information to routine tests (P {\textless} .00001) than {CSF} (P = .32) or {MR} imaging (P = .08).
Conclusion: Imaging and {CSF} biomarkers can improve prediction of conversion from {MCI} to {AD} compared with baseline clinical testing. {FDG} {PET} appears to add the greatest prognostic information. © {RSNA}, 2012 Supplemental material: http://radiology.rsna.org/lookup/suppl/doi:10.1148/radiol.12120010/-{/DC1}},
	language = {en},
	number = {2},
	urldate = {2013-03-08},
	journal = {Radiology},
	author = {Shaffer, Jennifer L. and Petrella, Jeffrey R. and Sheldon, Forrest C. and Choudhury, Kingshuk Roy and Calhoun, Vince D. and Coleman, R. Edward and Doraiswamy, P. Murali},
	month = feb,
	year = {2013},
	pages = {583--591},
	file = {Full Text PDF:/home/ben/.zotero/zotero/yu6itdki.default/zotero/storage/3B5NTDQ4/Shaffer et al. - 2013 - Predicting Cognitive Decline in Subjects at Risk f.pdf:application/pdf;Snapshot:/home/ben/.zotero/zotero/yu6itdki.default/zotero/storage/B2TG96TT/583.html:text/html}
