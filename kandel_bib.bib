	keywords = {Artificial Intelligence (incl. Robotics), Computer Graphics, Image Processing and Computer Vision, Pattern Recognition},
	pages = {404--417},
	file = {Full Text PDF:/home/ben/.zotero/zotero/yu6itdki.default/zotero/storage/S3B2GUV9/Bay et al. - 2006 - SURF Speeded Up Robust Features.pdf:application/pdf;Snapshot:/home/ben/.zotero/zotero/yu6itdki.default/zotero/storage/ZVTXEMNA/11744023_32.html:text/html}
