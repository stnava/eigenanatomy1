	keywords = {Artificial Intelligence (incl. Robotics), Computer Graphics, Health Informatics, Image Processing and Computer Vision, Imaging / Radiology, Pattern Recognition},
	pages = {723--730},
	file = {Full Text PDF:/home/ben/.zotero/zotero/yu6itdki.default/zotero/storage/3GWVJZSV/Gaonkar and Davatzikos - 2012 - Deriving Statistical Significance Maps for SVM Bas.pdf:application/pdf;Snapshot:/home/ben/.zotero/zotero/yu6itdki.default/zotero/storage/QVITMSRV/10.html:text/html}
